  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Docker: Compose",
      "type": "python",
      "request": "launch",
      "program": "docker",
      "console": "integratedTerminal",
      "preLaunchTask": "docker-build",
      "args": [
        "ps"
      ]
    },
    {
      "name": "Python: do-request.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\do_request.py",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}\\.venv",
      "pythonPath": "${config:python.pythonPath}",
      "args": [
        "100"
      ]
    }
  ]
}